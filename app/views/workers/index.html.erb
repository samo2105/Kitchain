<p id="notice"><%= notice %></p>

<h1>Workers</h1>

<table>
  <thead>
    <tr>
      <th>Office</th>
      <th>Name</th>
      <th>Rol</th>
      <th>Adress</th>
      <th>Number</th>
      <th>Admin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @workers.each do |worker| %>
      <tr>
        <td><%= worker.office %></td>
        <td><%= worker.name %></td>
        <td><%= worker.rol %></td>
        <td><%= worker.adress %></td>
        <td><%= worker.number %></td>
        <td><%= worker.admin %></td>
        <td><%= link_to 'Show', worker %></td>
        <td><%= link_to 'Edit', edit_worker_path(worker) %></td>
        <td><%= link_to 'Destroy', worker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Worker', new_worker_path %>
