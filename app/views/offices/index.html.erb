<%= render 'commerces/shared/navbarcommerce' %>
<br><br>

<h1>Offices</h1>

<table>
  <thead>
    <tr>
      <th>Commerce</th>
      <th>Name</th>
      <th>Adress</th>
      <th>Number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @offices.each do |office| %>
      <tr>
        <td><%= office.commerce %></td>
        <td><%= office.name %></td>
        <td><%= office.adress %></td>
        <td><%= office.number %></td>
        <td><%= link_to 'Show', office %></td>
        <td><%= link_to 'Edit', edit_office_path(office) %></td>
        <td><%= link_to 'Destroy', office, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Office', new_office_path %>
